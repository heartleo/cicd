pipeline {
   agent any

    tools {
        go 'go1.18'
    }

    environment {

    }

   stages {
      stage('pull code') {
         steps {
            echo 'pull code'
            git branch: 'main', credentialsId: 'heartleo-github-token', url: 'https://github.com/heartleo/cicd.git'
         }
      }
      stage('build code') {
         steps {
            echo 'build code'
            sh 'go version'
         }
      }
      stage('deploy project') {
         steps {
            echo 'deploy project'
         }
      }
   }
}
